{
  "name": "prompt-autocompletion",
  "description": "Prompt that autocompletes as you type. Can be used standalone or with a prompt system like [enquirer]",
  "version": "0.1.2",
  "homepage": "https://github.com/enquirer/prompt-autocompletion",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "enquirer/prompt-autocompletion",
  "bugs": {
    "url": "https://github.com/enquirer/prompt-autocompletion/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "figures": "^1.7.0",
    "log-utils": "^0.2.1",
    "prompt-base": "^0.5.0",
    "prompt-choices": "^0.4.0",
    "readline-utils": "^0.1.2",
    "terminal-paginator": "^0.2.1"
  },
  "devDependencies": {
    "enquirer": "^0.3.1",
    "gulp-format-md": "^0.1.11"
  },
  "keywords": [
    "auto",
    "autocomplete",
    "autocompletion",
    "complete",
    "generategenerator",
    "prompt"
  ],
  "verb": {
    "toc": false,
    "layout": "common-minimal",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "related": {
      "list": [],
      "prompts": [
        "prompt-checkbox",
        "prompt-confirm",
        "prompt-editor",
        "prompt-expand",
        "prompt-list",
        "prompt-password",
        "prompt-radio",
        "prompt-rawlist"
      ]
    },
    "reflinks": [
      "enquirer",
      "verb",
      "verb-generate-readme"
    ]
  }
}
